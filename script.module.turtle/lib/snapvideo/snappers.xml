<?xml version="1.0" encoding="UTF-8"?>
<snappers xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="https://code.google.com/p/apple-tv2-xbmc/snappers/ snappers.xsd"
	xmlns="https://code.google.com/p/apple-tv2-xbmc/snappers/">

	<snapper module="snapvideo.Dailymotion" function="retrievePlaylistVideoItems"
		playlist="true" enabled="true" desc="www.dailymotion.com video playlist snapper">
		<video-id regex="dailymotion.com/playlist/(.+?)_" />
		<video-id
			regex="dailymotion.com/widget/jukebox\?list\[\]\=\%2Fplaylist\%2F(.+?)&amp;" />
	</snapper>

	<snapper module="snapvideo.YouTube" function="retrievePlaylistVideoItems"
		playlist="true" enabled="true" desc="www.youtube.com video playlist snapper">
		<video-id regex="youtube.com/playlist\?list=PL(.+?)&amp;" />
		<video-id regex="youtube.com/playlist\?list=(.+?)&amp;" />
		<video-id regex="youtube.com/p/(.+?)/" />
		<video-id regex="youtube.com/p/(.+?)\?" />
		<video-id regex="youtube.com/p/(.+?)&amp;" />
		<video-id regex="youtube.com/feeds/api/playlists/(.+?)&amp;" />
	</snapper>

	<snapper module="snapvideo.YouTube" function="retrieveReloadedPlaylistVideoItems"
		playlist="true" enabled="true" desc="www.youtube.com video playlist snapper">
		<video-id regex="youtubereloaded.com/playlists/(.+?)&amp;" />
	</snapper>

	<snapper module="snapvideo.Dailymotion" function="retrieveVideoInfo"
		enabled="true" desc="www.dailymotion.com video hosting website">
		<video-id regex="dailymotion.com/video/(.+?)_" />
		<video-id regex="dailymotion.com/video/(.+?)&amp;" />
		<video-id regex="dailymotion.com/embed/video/(.+?)&amp;" />
		<video-id regex="dailymotion.com/swf/video/(.+?)&amp;" />
		<video-id regex="dailymotion.com/swf/(.+?)&amp;" />
		<video-id regex="dailymotion.com/swf/(.+?)&quot;" />
		<video-id regex="dailymotion.com/[a-zA-Z0-9]*\#videoId=(.+?)&amp;" />
	</snapper>
	<snapper module="snapvideo.YouTube" function="retrieveVideoInfo"
		enabled="true" desc="www.youtube.com video hosting website">
		<video-id regex="youtube.com/watch\?feature\=player_embedded&amp;v=(.+?)&amp;" />
		<video-id regex="youtube.com/watch\?v=(.+?)&amp;" />
		<video-id regex="youtube.com/embed/(.+?)&amp;" />
		<video-id regex="href=&quot;http://youtu.be/(.+?)&quot;" />
		<video-id regex="\&#39;VIDEO_ID\&#39;: &quot;(.+?)&quot;" />
		<video-id regex="youtube.com/v/(.+?)\?" />
		<video-id regex="youtube.com/v/(.+?)&amp;" />
		<video-id regex="youtube.com/v/(.+?)&quot;" />
		<video-id regex="youtube.com/watch\?[a-zA-Z0-9&amp;=]*&amp;v=(.+?)&amp;" />
	</snapper>
